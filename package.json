{
  "name": "angular-http-etag",
  "version": "0.2.1",
  "description": "Angular $http interceptor enabling use of ETags",
  "homepage": "https://github.com/shaungrady/angular-http-etag#readme",
  "author": "Shaun Grady",
  "license": "MIT",
  "main": "./src",
  "scripts": {
    "umd": "browserify -e . -s angularHttpEtags -t [exposify --expose [ --angular=angular ] ] -p derequire/plugin > ./release/angular-http-etags.js",
    "start": "mochify --watch",
    "phantom": "mochify",
    "coverage": "mochify --plugin [ mochify-istanbul --report lcov --dir './coverage' ]",
    "test": "npm run phantom && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaungrady/angular-http-etag.git"
  },
  "keywords": [
    "angular",
    "$http",
    "$cacheFactory",
    "etag",
    "cache",
    "interceptor"
  ],
  "bugs": {
    "url": "https://github.com/shaungrady/angular-http-etag/issues"
  },
  "dependencies": {
    "browserify-shim": "^3.8.8",
    "query-string": "^2.3.0"
  },
  "peerDependencies": {
    "angular": ">=1.3 <1.5"
  },
  "devDependencies": {
    "angular": "~1.4.0",
    "angular-mocks": "~1.4.0",
    "browserify": "^10.2.4",
    "chai": "^3.0.0",
    "chai-spies": "^0.6.0",
    "derequire": "^2.0.0",
    "exposify": "^0.4.3",
    "gulp": "^3.9.0",
    "mochify": "^2.9.0",
    "mochify-istanbul": "^2.3.0",
    "phantom": "^0.7.2"
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "coverage",
    "test"
  ]
}
