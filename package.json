{
  "name": "angular-http-etag",
  "version": "0.1.0",
  "description": "Angular $http interceptor enabling use of ETags",
  "homepage": "https://github.com/shaungrady/angular-http-etag#readme",
  "author": "Shaun Grady",
  "license": "MIT",
  "main": "./src",
  "scripts": {
    "umd": "browserify -e . -s angularHttpEtags -t [exposify --expose [ --angular=angular ] ] -p derequire/plugin > ./release/angular-http-etags.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaungrady/angular-http-etag.git"
  },
  "keywords": [
    "angular",
    "$http",
    "etag"
  ],
  "bugs": {
    "url": "https://github.com/shaungrady/angular-http-etag/issues"
  },
  "dependencies": {
    "babelify": "^6.1.2",
    "browserify-shim": "^3.8.8",
    "query-string": "^2.3.0"
  },
  "peerDependencies": {
    "angular": ">=1.3 <1.5"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "derequire": "^2.0.0",
    "exposify": "^0.4.3",
    "gulp": "^3.9.0",
    "standard": "^4.2.1"
  }
}
